	month = jan,
	year = {1997},
	keywords = {auditory cortex, blood oxygenation level-dependent, BOLD, Functional magnetic resonance imaging, tonotopy},
	pages = {18--25},
	file = {Full Text PDF:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/UU4MRNW2/Wessinger et al. - 1997 - Tonotopy in human auditory cortex examined with fu.pdf:application/pdf;Snapshot:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/S6DEJW68/abstract\;jsessionid=1B9C3629F3272E5EF73243F41392F69B.html:text/html}

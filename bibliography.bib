@article{wessinger_tonotopy_1997,
	title = {Tonotopy in human auditory cortex examined with functional magnetic resonance imaging},
	volume = {5},
	copyright = {Copyright © 1997 Wiley-Liss, Inc.},
	issn = {1097-0193},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/(SICI)1097-0193(1997)5:1<18::AID-HBM3>3.0.CO;2-Q/abstract},
	doi = {10.1002/(SICI)1097-0193(1997)5:1<18::AID-HBM3>3.0.CO;2-Q},
	abstract = {Tonotopic organization within the human auditory cortex was investigated with functional magnetic resonance imaging (fMRI) using the blood oxygenation level-dependent (BOLD) contrast mechanism. Single-frequency pulsed tones were alternated with no-tone conditions to elicit stimulus-specific functional activity. Differential frequency-specific activity was imaged within the auditory cortex Activations for high-frequency tones were located more posteriorly and medially than those for low-frequency tones. Such a pattern is consistent with descriptions of tonotopic organization suggested by other nonneuroimaging methodologies used with human and nonhuman primates. Furthermore, these results demonstrate that fMRI can be used to reliably investigate functional organization of the human auditory cortex. Hum. Brain Mapping 4:18–25, 1997. © 1997 Wiley-Liss, Inc.},
	language = {en},
	number = {1},
	urldate = {2016-01-18},
	journal = {Human Brain Mapping},
